        "hardware/intel/external/opencl/igc/include/IGC/AdaptorOCL/ocl_igc_shared/device_enqueue",
        "hardware/intel/external/opencl/igc/include/IGC/AdaptorOCL/ocl_igc_shared/executable_format",
        "hardware/intel/external/opencl/neo/device/include",
        "hardware/intel/external/opencl/neo/third_party",
        "hardware/intel/external/opencl/neo/third_party/gtest",
        "hardware/intel/external/opencl/neo/third_party/opencl_headers",
        "hardware/intel/external/opencl/neo/third_party/opengl_headers",
        "hardware/intel/external/opencl/neo/third_party/source_level_debugger",
        "hardware/intel/external/opencl/neo/third_party/uapi",
        "hardware/intel/external/opencl/neo/runtime/sku_info/definitions",
        "hardware/intel/external/opencl/neo/runtime/os_interface/definitions",
