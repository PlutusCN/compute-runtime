cc_defaults {
    name: "neo-defaults",

    vendor_available: true,

    cflags: [
        
    ],

    cppflags: [
        "-Wno-error",
        "-fexceptions",
        "-fno-strict-aliasing",
        "-msse",
        "-msse2",
        "-msse3",
        "-mssse3",
        "-msse4",
        "-msse4.1",
        "-msse4.2",
        "-DBIONIC_IOCTL_NO_SIGNEDNESS_OVERLOAD",
